<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net5.0</TargetFramework>
        <PackageVersion>0.1.0</PackageVersion>
        <Title>Negum.SDL</Title>
        <Authors>The Negum Project</Authors>
        <Copyright>MIT License</Copyright>
        <PackageProjectUrl>https://github.com/TheNegumProject/Negum.SDL</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/TheNegumProject/Negum.SDL</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/TheNegumProject/Negum.SDL</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <Company>The Negum Project</Company>
        <NeutralLanguage>en</NeutralLanguage>
        <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Negum.Core" Version="0.2021.417.1144" />
      <PackageReference Include="Negum.Game" Version="0.2021.419.623" />
      <PackageReference Include="ppy.SDL2-CS" Version="1.0.225-alpha" />
    </ItemGroup>

</Project>
